.alert-box.success {
    background-color: #DFF0D8;
    border-color: #D6E9C6;
    box-shadow: 1px 4px 2px 0 #DDDDDD, 0 1px 2px 0 #FFFFFF inset;
    color: #468847;
}

.alert-box.danger, .alert-box.error, .alert-box.fail {
    background-color: #F2DEDE;
    border-color: #EED3D7;
    box-shadow: 1px 4px 2px 0 #DDDDDD, 0 1px 2px 0 #FF624D inset;
    color: #B94A48;
}
